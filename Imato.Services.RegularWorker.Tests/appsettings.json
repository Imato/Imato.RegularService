{
  "ConnectionStrings": {
    "postgres": "Host=localhost;Port=5432;Database=unit_tests;Pooling=true;MinPoolSize=10;MaxPoolSize=100;"
  },
  "Environment": {
    "DbUser": "UNIT_TESTS_USER",
    "DbUserPassword": "UNIT_TESTS_PASSWORD"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    },
    "DbLogger": {
      "Options": {
        "ConnectionString": "Host=localhost;Port=5432;Database=unit_tests;Pooling=true;MinPoolSize=10;MaxPoolSize=100;",
        "Table": "log.logs",
        "Columns": "app, dt, level, message, host_name, source, user_name",
        "Environment": {
          "DbUser": "UNIT_TESTS_USER",
          "DbUserPassword": "UNIT_TESTS_PASSWORD"
        }
      }
    }
  }
}